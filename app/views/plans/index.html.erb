<input type="text" class="four columns" placeholder="Search for a location">

<div class="u-cf planner">
  <div class="three columns plans">
    <header>Journey Planner</header>
    <div class="contents">
      <% current_user.plans.each do |plan| %>
      <div class="plan">
        <p><%= plan.address %></p>
        <div class="events">
          <header>Events</header>
          <div class="items">
            <% plan.saved_events.each do |saved_event| %>
            <div>
              <div><%= saved_event.event.title %></div>
              <div><%= saved_event.event.info %></div>
            </div>
            <% end %>
          </div>
        </div>
      </div>
      <% end %>
    </div>
  </div>
  <div class="six columns map">Map goes here</div>
  <div class="three columns events-eats">Hello</div>
</div>
